!!! 5
html
  head
      meta(charset='utf-8')
      title MacGyver Example
      link(href='//netdna.bootstrapcdn.com/twitter-bootstrap/2.2.2/css/bootstrap-combined.min.css', rel='stylesheet')
      link(href='//fonts.googleapis.com/css?family=Montserrat', rel='stylesheet', type='text/css')
      link(href='css/vendor.css', rel='stylesheet')
      link(href='css/MacGyver.css', rel='stylesheet')

  body(ng-app="Mac", data-spy="scroll", data-target="#nav", data-offset="54")
    #top-nav
      .container
        a#logo(href="index.html"): h2 MacGyver
        ul.nav-menu
          li#directives-nav: a(href="index.html") Directives
          li#tableview-nav: a(href="tableview.html") Table View
          li#filters-nav: a(href="filters.html") Filters
          li#events-nav: a(href="events.html") Events
          li#utils-nav: a(href="util.html") Utilities

    block header

    .container
      block content

    footer.footer
      .container
        p Built by the Dev team at&nbsp;
          a(href="http://www.shift.com") SHIFT.com
        p Code licensed under&nbsp;
          a(href="https://github.com/StartTheShift/MacGyver/blob/master/LICENSE") MIT
      ul.footer-links
        li: a(href="https://github.com/StartTheShift/MacGyver/issues") Issues

    script(src="//cdnjs.cloudflare.com/ajax/libs/jquery/1.10.1/jquery.min.js", type='text/javascript')
    script(src="//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.4.3/underscore-min.js", type="text/javascript")
    script(src="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/2.2.2/bootstrap.js", type="text/javascript")
    script(src="//cdnjs.cloudflare.com/ajax/libs/prettify/r298/prettify.js", type="text/javascript")
    script(src='js/vendor.js', type="text/javascript")
    script(src='js/macgyver.js', type="text/javascript")
    block scripts
